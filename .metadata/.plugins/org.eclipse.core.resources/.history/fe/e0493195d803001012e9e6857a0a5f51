package main;

import entity.MayTinh;

public class Program_MayTinh {
    public static void main(String[] args) {
        MayTinh mt = new MayTinh("DELL", "MY", 2000, 1250000, 1);
        MayTinh.CPU cpu = mt.new CPU(120.0, "ABC", mt); 
        MayTinh.RAM ram = mt.new RAM(6, 120, mt);
        MayTinh.ROM rom = mt.new ROM(4, 120, mt);
        System.out.println(mt);
        System.out.println(cpu);
        System.out.println(ram);
         System.out.println(rom);
    }
}
