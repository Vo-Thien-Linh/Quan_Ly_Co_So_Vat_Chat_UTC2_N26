package entity;

public class MayTinh {
	private String ten;
	private String nguonGoc;
	private int namSX;
	private double gia;
	private int soLuong;
	public MayTinh(String ten, String nguonGoc, int namSX, double gia, int soLuong) {
		super();
		this.ten = ten;
		this.nguonGoc = nguonGoc;
		this.namSX = namSX;
		this.gia = gia;
		this.soLuong = soLuong;
	}
	public String getTen() {
		return ten;
	}
	public void setTen(String ten) {
		this.ten = ten;
	}
	public String getNguonGoc() {
		return nguonGoc;
	}
	public void setNguonGoc(String nguonGoc) {
		this.nguonGoc = nguonGoc;
	}
	public int getNamSX() {
		return namSX;
	}
	public void setNamSX(int namSX) {
		this.namSX = namSX;
	}
	public double getGia() {
		return gia;
	}
	public void setGia(double gia) {
		this.gia = gia;
	}
	public int getSoLuong() {
		return soLuong;
	}
	public void setSoLuong(int soLuong) {
		this.soLuong = soLuong;
	}
	
	public static class ROM{
		private int dungLuong;
		private double tocDo;
		public ROM(int dungLuong, double tocDo) {
			super();
			this.dungLuong = dungLuong;
			this.tocDo = tocDo;
		}
		public int getDungLuong() {
			return dungLuong;
		}
		public void setDungLuong(int dungLuong) {
			this.dungLuong = dungLuong;
		}
		public double getTocDo() {
			return tocDo;
		}
		public void setTocDo(double tocDo) {
			this.tocDo = tocDo;
		}
		
	}
	
	public static class CPU{
		private double xungNhip;
		private String nhaSX;
		public CPU(double xungNhip, String nhaSX) {
			super();
			this.xungNhip = xungNhip;
			this.nhaSX = nhaSX;
		}
		public double getXungNhip() {
			return xungNhip;
		}
		public void setXungNhip(double xungNhip) {
			this.xungNhip = xungNhip;
		}
		public String getNhaSX() {
			return nhaSX;
		}
		public void setNhaSX(String nhaSX) {
			this.nhaSX = nhaSX;
		}
		
		
	}
	
	public static class RAM{
		private int dungLuong;
		private double tocDo;
		public RAM(int dungLuong, double tocDo) {
			super();
			this.dungLuong = dungLuong;
			this.tocDo = tocDo;
		}
		public int getDungLuong() {
			return dungLuong;
		}
		public void setDungLuong(int dungLuong) {
			this.dungLuong = dungLuong;
		}
		public double getTocDo() {
			return tocDo;
		}
		public void setTocDo(double tocDo) {
			this.tocDo = tocDo;
		}
		
	}
}
